{
    "presets": ["@babel/preset-env"],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["module-resolver", {
            "root": ["./src"],
            "alias": {
                "modules/*": "./src/modules",
                "settings/*": "./settings"
            }
        }]
    ],
    "ignore": [
        "node_modules"
    ]
}